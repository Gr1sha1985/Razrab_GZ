	Раздел1Прил1.Очистить();
	ОчиститьПоляСтруктурыПоказателей(ПоказателиОтчета, СтруктураПолейПрил1Подразделы1и2);
	ОчиститьПоляСтруктурыПоказателей(ПоказателиОтчета, СтруктураПолейПрил1Подраздел3_1);
	ОчиститьПоляСтруктурыПоказателей(ПоказателиОтчета, СтруктураПолейПрил1Подраздел3_2);
	
	Раздел1Прил11 = Контейнер.Раздел1Прил1_1.Строки[0].ДанныеМногострочныхЧастей.П00111М1.Строки;
	СтруктураПолейПрил11 = ОбщегоНазначения.СкопироватьРекурсивно(Раздел1Прил11[0].Данные);
	Раздел1Прил11.Очистить();
	ОчиститьПоляСтруктурыПоказателей(ПоказателиОтчета, СтруктураПолейПрил11);
	
	Раздел1Прил2 = Контейнер.Раздел1Прил2.Строки[0];
	Раздел1Прил2ПоТарифам = Раздел1Прил2.ДанныеМногострочныхЧастей.П00012М1.Строки;
	СтруктураПолейПрил2 = ОбщегоНазначения.СкопироватьРекурсивно(Раздел1Прил2ПоТарифам[0].Данные);
	Раздел1Прил2ПоТарифам.Очистить();
	ОчиститьПоляСтруктурыПоказателей(ПоказателиОтчета, СтруктураПолейПрил2);
	ОчиститьПоляСтруктурыПоказателей(ПоказателиОтчета, Раздел1Прил2.Данные);
	
	Раздел1Прил8 = Контейнер.Раздел1Прил8.Строки[0].ДанныеМногострочныхЧастей.П00018М1.Строки;
