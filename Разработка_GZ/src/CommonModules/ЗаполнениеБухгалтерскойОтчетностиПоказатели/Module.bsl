	КонецЕсли;
	
	ДобавитьПоказателиБО_ФормаОтчета2011Кв4_ОИК(ПоказателиОтчета, ПараметрыОтчета);
	ДобавитьПоказателиБО_ФормаОтчета2011Кв4_ОДДС(ПоказателиОтчета, ПараметрыОтчета);
	
	ДобавитьПоказателиБО_ФормаОтчета2011Кв4_Пояснения1(ПоказателиОтчета, ПараметрыОтчета);
	ДобавитьПоказателиБО_ФормаОтчета2011Кв4_Пояснения2(ПоказателиОтчета, ПараметрыОтчета);
	ДобавитьПоказателиБО_ФормаОтчета2011Кв4_Пояснения4(ПоказателиОтчета, ПараметрыОтчета);
	ДобавитьПоказателиБО_ФормаОтчета2019Кв4_Пояснения5(ПоказателиОтчета, ПараметрыОтчета);
