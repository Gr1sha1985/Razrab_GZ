Процедура ДобавитьПоказателиБО_ФормаОтчета2011Кв1(ПоказателиОтчета) Экспорт
	
	// Бухгалтерский баланс.
	Префикс = "П00010";
	Графы = "04, 05, 06";
	
	Строки = "1110, 1120, 1130, 1140, 1150, 1160, 1170,
	         |1210, 1220, 1230, 1240, 1250, 1260,
	         |1310, 1320, 1340, 1350, 1360, 1370,
	         |1410, 1420,
	         |1510, 1520, 1530, 1540, 1550";
	ВставитьПоказателиВСтруктуру(ПоказателиОтчета, Префикс, Строки, Графы);
	
	// Отчет о прибылях и убытках.
	Префикс = "П00010";
	Графы = "04, 05";
	
	Строки = "2110, 2120, 2210, 2220, 2310, 2320, 2330,
	         |2340, 2350, 2410, 2421, 2430, 2450, 2460";
	ВставитьПоказателиВСтруктуру(ПоказателиОтчета, Префикс, Строки, Графы);
	
КонецПроцедуры

