Процедура УстановитьЗначениеПоказателяПоСоставляющимСОНКО(СоставляющиеПоказателя, КонтейнерОтчета, КодСтроки, Графа)
	
	ЗначениеПоказателя = ОпределитьЗначениеПоСоставляющимСОНКО(СоставляющиеПоказателя, КонтейнерОтчета, КодСтроки, Графа);
	КонтейнерОтчета[КодСтроки + Графа] = КонтейнерОтчета[КодСтроки + Графа] + ЗначениеПоказателя;
	
КонецПроцедуры

Процедура УстановитьЗначениеВычитаемогоПоказателяПоСоставляющимСОНКО(СоставляющиеПоказателя, КонтейнерОтчета, КодСтроки, Графа)
	
	ЗначениеПоказателя = ОпределитьЗначениеПоСоставляющимСОНКО(СоставляющиеПоказателя, КонтейнерОтчета, КодСтроки, Графа);
	КонтейнерОтчета[КодСтроки + Графа] = КонтейнерОтчета[КодСтроки + Графа] - ЗначениеПоказателя;
	
КонецПроцедуры

Функция ОпределитьЗначениеПоСоставляющимСОНКО(СоставляющиеПоказателя, КонтейнерОтчета, КодСтроки, Графа)
	
	ИтогПоСоставляющим = ИтогПоСоставляющим(СоставляющиеПоказателя);
	
	Если Лев(КодСтроки, 6) = "П00001" Тогда // Под КодСтроки здесь понимается условное значение - часть номера ячейки
		Если Графа = "04" Тогда
			Если КонтейнерОтчета.Свойство(КодСтроки + "03") Тогда
				КонтейнерОтчета[КодСтроки + "03"] = ИтогПоСоставляющим.КодСтроки;
			КонецЕсли;
		КонецЕсли;
	Иначе
		Если Графа = "03" Тогда
			Если КонтейнерОтчета.Свойство(КодСтроки + "02") Тогда
				КонтейнерОтчета[КодСтроки + "02"] = ИтогПоСоставляющим.КодСтроки;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	
	Возврат ИтогПоСоставляющим.Сумма;
	
КонецФункции

Функция ИтогПоСоставляющим(СоставляющиеПоказателя)
	
	ОписаниеИтога = Новый Структура;
	ОписаниеИтога.Вставить("КодСтроки", "");
	ОписаниеИтога.Вставить("Сумма",     0);
	
	МаксимальнаяСоставляющая = Неопределено;
	Для Каждого Составляющая Из СоставляющиеПоказателя Цикл
		ОписаниеИтога.Сумма = ОписаниеИтога.Сумма + Составляющая.Значение;
		Если МаксимальнаяСоставляющая = Неопределено Или Составляющая.Значение > МаксимальнаяСоставляющая Тогда
			МаксимальнаяСоставляющая = Составляющая.Значение;
			ОписаниеИтога.КодСтроки = Составляющая.Ключ;
		КонецЕсли;
	КонецЦикла;
	
	Возврат ОписаниеИтога;
	
КонецФункции

