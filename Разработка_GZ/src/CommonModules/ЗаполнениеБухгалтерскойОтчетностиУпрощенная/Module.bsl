Функция ОпределитьЗначениеПоСоставляющим(СоставляющиеПоказателя, КонтейнерОтчета, КодСтроки, Графа)
	
	ЗначениеПоказателя = 0;
	МаксимальнаяСоставляющая = Неопределено;
	КодПоказателя = "";
	Для Каждого Составляющая Из СоставляющиеПоказателя Цикл
		ЗначениеПоказателя = ЗначениеПоказателя + Составляющая.Значение;
		Если МаксимальнаяСоставляющая = Неопределено ИЛИ Составляющая.Значение > МаксимальнаяСоставляющая Тогда
			МаксимальнаяСоставляющая = Составляющая.Значение;
			КодПоказателя = Составляющая.Ключ;
		КонецЕсли;
	КонецЦикла;
	
	Если Графа = "03" Тогда
		Если КонтейнерОтчета.Свойство(КодСтроки + "02") Тогда
			КонтейнерОтчета[КодСтроки + "02"] = КодПоказателя;
		КонецЕсли;
	КонецЕсли;
	
	Возврат ЗначениеПоказателя;
	
КонецФункции

Процедура ЗаполнитьЯчейкиОЦИС(Контейнер, ТаблицаРасшифровки, ДанныеЗаполнения, Раздел, КодГрафы)
	
	// Вызывающий код не обеспечивает инициализацию объекта
	Если ТаблицаРасшифровки = Неопределено Тогда
		ТаблицаРасшифровки = ЗаполнениеРасшифровкаРегламентированнойОтчетности.НовыйРасшифровка();
	КонецЕсли;
	
	РазделКонтейнера = Контейнер[Раздел];
	ПрефиксРаздела   = ЗаполнениеБухгалтерскойОтчетностиПоказатели.ПрефиксЯчейкиОЦИС();
	
	Для Каждого ОписаниеПоказателя Из ДанныеЗаполнения.Показатели Цикл
		ЗаполнениеБухгалтерскойОтчетностиПоказатели.УстановитьЗначениеПоказателя(
			РазделКонтейнера,
			ПрефиксРаздела,
			ОписаниеПоказателя.Ключ,
			КодГрафы,
			ОписаниеПоказателя.Значение);
	КонецЦикла;
	
	Для Каждого ИсходнаяЗапись Из ДанныеЗаполнения.Расшифровка Цикл
		
		Запись = ТаблицаРасшифровки.Добавить();
		
		ЗаполнитьЗначенияСвойств(Запись, ИсходнаяЗапись);
		
		Запись.ИмяРаздела    = Раздел;
		Запись.ИмяПоказателя = ЗаполнениеБухгалтерскойОтчетностиПоказатели.НомерЯчейки(
			ПрефиксРаздела,
			ИсходнаяЗапись.ИмяПоказателя,
			КодГрафы);
			
	КонецЦикла;
	
КонецПроцедуры

Процедура УкрупнитьПоказателиОЦИС(ДанныеЗаполнения, Контейнер, ТаблицаРасшифровки, ИмяПериода, КодГрафыСуммы, Раздел)
	
	ПрефиксРаздела = ЗаполнениеБухгалтерскойОтчетностиПоказатели.ПрефиксЯчейкиОЦИС();
	
	// Расшифровка укрупненного показателя включает расшифровку составляющих (и в текущем и в предыдущем периоде)
	УкрупненныеСтроки = ЗаполнениеБухгалтерскойОтчетностиПоказатели.УкрупненныеСтрокиУпрощеннойФормыОЦИС();
	
	ЗаголовкиУкрупненныхСтрок = ЗаполнениеБухгалтерскойОтчетностиПоказатели.ЗаголовкиУкрупненныхСтрокУпрощеннойФормыОЦИС();
	
	Для Каждого ИсходнаяЗапись Из ДанныеЗаполнения.Расшифровка Цикл
		
		УсловныйКодУкрупненнойСтроки = УкрупненныеСтроки[ИсходнаяЗапись.ИмяПоказателя];
		Если УсловныйКодУкрупненнойСтроки = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		Запись = ТаблицаРасшифровки.Добавить();
		ЗаполнитьЗначенияСвойств(Запись, ИсходнаяЗапись);
		Запись.ИмяРаздела    = Раздел;
		Запись.ИмяПоказателя = ЗаполнениеБухгалтерскойОтчетностиПоказатели.НомерЯчейки(
			ПрефиксРаздела,
			УсловныйКодУкрупненнойСтроки,
			КодГрафыСуммы);
			
		// Включим в описание номера строк составляющих, чтобы было понятно,
		// как определен код строки
		ЭлементыОписанияСлагаемого = Новый Массив;
		ЭлементыОписанияСлагаемого.Добавить(Запись.НаименованиеПоказателя);
		ЭлементыОписанияСлагаемого.Добавить(Запись.НаименованиеСлагаемого);
		Запись.НаименованиеСлагаемого = СтрСоединить(ЭлементыОписанияСлагаемого, Символы.ПС);
		
		// Заголовок укрупненной строки должен содержать указание на укрупненную строку
		ЗаголовокУкрупненнойСтроки = ЗаголовкиУкрупненныхСтрок[УсловныйКодУкрупненнойСтроки];
		Если ЗначениеЗаполнено(ЗаголовокУкрупненнойСтроки) Тогда
			Запись.НаименованиеПоказателя = СтрШаблон(НСтр("ru = 'Строка ""%1""'"), ЗаголовокУкрупненнойСтроки);
		КонецЕсли;
		
	КонецЦикла;
	
	// Код строки определяется по данным за текущий период
	Если ИмяПериода <> "ТекущийПериод" Тогда
		Возврат;
	КонецЕсли;
	
	СоставныеПоказатели = Новый Соответствие;
	
	Для Каждого ОписаниеСтроки Из УкрупненныеСтроки Цикл
		
		Составляющие = СоставныеПоказатели[ОписаниеСтроки.Значение];
		Если Составляющие = Неопределено Тогда
			Составляющие = Новый Соответствие;
			СоставныеПоказатели.Вставить(ОписаниеСтроки.Значение, Составляющие);
		КонецЕсли;
		
		ЗначениеПоказателя = ДанныеЗаполнения.Показатели[ОписаниеСтроки.Ключ];
		Если Не ЗначениеЗаполнено(ЗначениеПоказателя) Тогда
			Продолжить;
		КонецЕсли;
		
		Составляющие.Вставить(ОписаниеСтроки.Ключ, ЗначениеПоказателя);
		
	КонецЦикла;
	
	КодыСтрокУкрупненныхПоказателей = Новый Соответствие;
	
	Для Каждого Составляющие Из СоставныеПоказатели Цикл
		
		ИтогиАгрегатногоПоказателя = ИтогПоСоставляющим(Составляющие.Значение);
		КодыСтрокУкрупненныхПоказателей.Вставить(Составляющие.Ключ, ИтогиАгрегатногоПоказателя.КодСтроки);
		ДанныеЗаполнения.Показатели.Вставить(Составляющие.Ключ, ИтогиАгрегатногоПоказателя.Сумма);
		
	КонецЦикла;
	
	// Поместим в контейнер код строки укрупненного показателя
	РазделКонтейнера  = Контейнер[Раздел];
	Для Каждого ОписаниеПоказателя Из КодыСтрокУкрупненныхПоказателей Цикл
		
		ЗаполнениеБухгалтерскойОтчетностиПоказатели.УстановитьЗначениеПоказателя(
			РазделКонтейнера,
			ПрефиксРаздела,
			ОписаниеПоказателя.Ключ,
			ЗаполнениеБухгалтерскойОтчетностиПоказатели.ГрафаКодСтрокиУпрощеннойФормыОЦИС(),
			ОписаниеПоказателя.Значение);
		
	КонецЦикла;
	
КонецПроцедуры

Процедура ЗаполнитьМПФормаОтчета2015Кв4_ОЦИС(Контейнер, ТаблицаРасшифровки, ПериодыОтчета, Организация)
	
	Упрощенная = Истина;
	Раздел     = "ОЦИС"; // В основной форме имя раздела отличается
	
	ГрафыОЦИС = ЗаполнениеБухгалтерскойОтчетностиПоказатели.ГрафыОЦИС(Упрощенная);
	
	Для Каждого ОписаниеПериода Из ПериодыОтчета Цикл
	
		ДанныеЗаполнения = ЗаполнениеБухгалтерскойОтчетностиОсновная.ДанныеЗаполненияОЦИС(
			ОписаниеПериода.Значение.Начало,
			ОписаниеПериода.Значение.Конец,
			Организация,
			Упрощенная);
			
		Если ДанныеЗаполнения = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		КодГрафы = ГрафыОЦИС[ОписаниеПериода.Ключ];
		
		УкрупнитьПоказателиОЦИС(ДанныеЗаполнения, Контейнер, ТаблицаРасшифровки, ОписаниеПериода.Ключ, КодГрафы, Раздел);
		
		ЗаполнитьЯчейкиОЦИС(Контейнер, ТаблицаРасшифровки, ДанныеЗаполнения, Раздел, КодГрафы);
		
	КонецЦикла;
	
КонецПроцедуры

#КонецОбласти

#Область ЗаполнениеОтчетовБОСОНКО

Процедура ЗаполнитьСОНКОФормаОтчета2012Кв4_БалансПоГрафе(КонтекстВыполнения, Контейнер)
	
	Если КонтекстВыполнения.КонтекстОтчета.КонецПериодаОтчета >= '2015-01-01' Тогда
		ЗаполнитьСОНКОФормаОтчета2012Кв4_БалансПоГрафе2015Кв1(КонтекстВыполнения, Контейнер);
	Иначе
		ЗаполнитьСОНКОФормаОтчета2012Кв4_БалансПоГрафе2012Кв4(КонтекстВыполнения, Контейнер);
	КонецЕсли;
	
КонецПроцедуры

Процедура ЗаполнитьСОНКОФормаОтчета2012Кв4_БалансПоГрафе2012Кв4(КонтекстВыполнения, Контейнер)
	
	Организация = КонтекстВыполнения.Организация;
	ТаблицаРасшифровки = КонтекстВыполнения.ТаблицаРасшифровки;
	
	Графа = КонтекстВыполнения.КонтекстОтчета.Графа;
	ЗаголовокГрафы = КонтекстВыполнения.КонтекстОтчета.ЗаголовокГрафы;
	НачалоПериодаОтчета = КонтекстВыполнения.КонтекстОтчета.НачалоПериодаОтчета;
	КонецПериодаОтчета  = КонтекстВыполнения.КонтекстОтчета.КонецПериодаОтчета;
	
	ОбщиеПараметрыРасшифровки = Новый Структура;
