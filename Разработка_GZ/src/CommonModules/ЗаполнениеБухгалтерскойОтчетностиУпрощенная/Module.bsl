	ДобавитьСтрокуРасшифровки(ТаблицаРасшифровки, КодСтроки + Графа, "", "+", "!СКД " + Счет("08.02").Код, СКД_08_02, "Баланс", НастройкиОСВПоСчету(ОбщиеПараметрыРасшифровки, Счет("08.02")));
