	ОБ_05__04_01 = ЗаполнениеБухгалтерскойОтчетностиИтоги.ОБ("05", "04.01", ОборотыМеждуСчетами);
	
	ОБ_04_01__83_01_2 = ЗаполнениеБухгалтерскойОтчетностиИтоги.ОБ("04.01", "83.01.2", ОборотыМеждуСчетами);
