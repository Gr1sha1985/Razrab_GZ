		Результат = Ложь;
		ТекстОшибки = НСтр("ru='Нет данных для выгрузки.'");
		СформироватьОшибку(ВыходныеПараметры, ТекстОшибки);
		Возврат Результат;
	КонецЕсли;
	
	РазмерПакета     = РазмерПакетаПоУмолчанию();
	ТекущийПакет     = 1;
	ЗаписьВПакете    = 0;
	ЗаписейВыгружено = 0;
