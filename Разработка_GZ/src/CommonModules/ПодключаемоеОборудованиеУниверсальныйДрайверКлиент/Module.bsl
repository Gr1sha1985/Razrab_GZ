		СформироватьОшибку(ВыходныеПараметры, ТекстОшибки);
		Возврат Результат;
	КонецЕсли;
	
	МенеджерОборудованияКлиент.СостояниеПроцесса(НСтр("ru='Подготовка данных...'"));
	
	РазмерПакета     = РазмерПакетаПоУмолчанию();
