		ВходныеПараметры  = Новый Массив();
		ВходныеПараметры.Добавить("Items");
		ВходныеПараметры.Добавить(Параметры.ТаблицаВыгрузкиТоваров);
		ВходныеПараметры.Добавить(Параметры.ПолнаяВыгрузка);
		
