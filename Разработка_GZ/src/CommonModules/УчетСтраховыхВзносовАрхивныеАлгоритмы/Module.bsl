	Запрос.Текст = ОсновнойТекст;
	МассивРезультатов = Запрос.ВыполнитьПакет();
	ВсегоРезультатов = МассивРезультатов.Количество();
	
	Результат = МассивРезультатов[ВсегоРезультатов - 3];
	Если Результат.Пустой() Тогда
		Возврат ;
	КонецЕсли;
	
	ТаблицаРезультата = Результат.Выгрузить();
	ПрефиксВывода = ТаблицаРезультата[0].ПрефиксВывода;
	
	Если Не ЗначениеЗаполнено(ПрефиксВывода) Тогда
		Возврат ;
	КонецЕсли;
	
	// Титульный лист 
	
	Контейнер.Титульный.Тариф = ТаблицаРезультата[0].КодТарифа;
	
	// Раздел 1 
	
	Запрос.Текст = ОстаткиНаНачалоПериодаТекст;
	ВывестиДанныеВТаблицуРВ3(ПрефиксВывода, Контейнер.Раздел12, Запрос.Выполнить().Выгрузить());
	ВывестиДанныеВТаблицуРВ3(ПрефиксВывода, Контейнер.Раздел12, МассивРезультатов[ВсегоРезультатов - 1].Выгрузить());
	
	// Раздел 2
	
	ВывестиДанныеВТаблицуРВ3(ПрефиксВывода, Контейнер.Раздел12, МассивРезультатов[ВсегоРезультатов - 2].Выгрузить());

	// Раздел 3
	Запрос.Текст = ЗарегистрированныеИсправленияТекст;
	ВременнаяТаблица = Запрос.Выполнить().Выгрузить();
	КолонкиРезультата = ВременнаяТаблица.Колонки;
	Для каждого Колонка Из КолонкиРезультата Цикл
		Колонка.Имя = СтрЗаменить(Колонка.Имя, ПрефиксВывода, "")
	КонецЦикла;
	Контейнер.Раздел3_ДанныеДопСтрок.Очистить();
	НомерСтроки = 1;
	Для каждого СтрокаДанных Из ВременнаяТаблица Цикл
		СтрокаКонтейнера = Контейнер.Раздел3_ДанныеДопСтрок.Добавить();
		ЗаполнитьЗначенияСвойств(СтрокаКонтейнера, СтрокаДанных);
		СтрокаКонтейнера.П000300000101 = НомерСтроки;
		НомерСтроки = НомерСтроки + 1;
	КонецЦикла;
	
КонецПроцедуры

