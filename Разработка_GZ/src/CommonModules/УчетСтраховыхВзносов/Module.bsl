	|	ВЫБОР
	|		КОГДА СведенияОДоходах.ЗастрахованФСС
	|			ТОГДА ИСТИНА
	|		КОГДА СведенияОДоходах.ЗастрахованФССкакИностранец
	|				И НЕ ОписаниеТарифов.ВыделятьДоходыИностранцевДляФСС
	|			ТОГДА ИСТИНА
	|		ИНАЧЕ ЛОЖЬ
	|	КОНЕЦ,
	|	ВЫБОР
	|		КОГДА СведенияОДоходах.ЗастрахованФССкакИностранец
	|				И ОписаниеТарифов.ВыделятьДоходыИностранцевДляФСС
	|			ТОГДА ИСТИНА
	|		ИНАЧЕ ЛОЖЬ
	|	КОНЕЦ,
	|	СведенияОДоходах.СКЕ,
	|	СведенияОДоходах.СтавкаНаНСиПЗ
	|
	|ИМЕЮЩИЕ
	|	(СУММА(СведенияОДоходах.Сумма) <> 0
	|		ИЛИ СУММА(СведенияОДоходах.Скидка) <> 0
	|		ИЛИ СУММА(СведенияОДоходах.БазаПФР) <> 0
	|		ИЛИ СУММА(СведенияОДоходах.БазаФОМС) <> 0
	|		ИЛИ СУММА(СведенияОДоходах.БазаФСС) <> 0
	|		ИЛИ СУММА(СведенияОДоходах.БазаФСС_НС) <> 0)";

	ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "ВТСведенияБезУточненияЗастрахованностиИностранцевВФСС", ИмяТаблицы);
	Если ОставлятьМесяцыБезДоходов Тогда
		НачальнаяПозиция = СтрНайти(ТекстЗапроса, "ИМЕЮЩИЕ");
		ТекстЗапроса = Лев(ТекстЗапроса, НачальнаяПозиция - 1);
	КонецЕсли;   
	
	Запрос.Текст = ТекстЗапроса + "
	|
	|ИНДЕКСИРОВАТЬ ПО
	|	ФизическоеЛицо
	|";
	Запрос.Выполнить();
	
	ЗарплатаКадры.УничтожитьВТ(МенеджерВременныхТаблиц, ТаблицыКУничтожению);
	
КонецПроцедуры

Функция НоваяСтрокаСОбнулениемРесурсов(ТаблицаЗначений, Ресурсы)
	
	НоваяСтрока = ТаблицаЗначений.Добавить();
	Для каждого Ресурс Из Ресурсы Цикл
		НоваяСтрока[Ресурс] = 0
	КонецЦикла; 
	
	Возврат НоваяСтрока
	
КонецФункции 

Процедура ВыделитьБазуМРОТНаОсновнойТариф(СтрокиМесяца, Результат, ОсновнойТариф)
	СтрокРаспределения = СтрокиМесяца.Количество();
	Для СчетчикРаспределения = 0 По СтрокРаспределения - 1 Цикл
		ИндексСтрокиРаспределения = СтрокРаспределения - 1 - СчетчикРаспределения;
		СтрокаРаспределения = СтрокиМесяца[ИндексСтрокиРаспределения];
		Если СтрокаРаспределения.БазаПФР = 0 И СтрокаРаспределения.БазаФОМС = 0 И СтрокаРаспределения.БазаФСС = 0 Тогда
			СтрокиМесяца.Удалить(ИндексСтрокиРаспределения)
		КонецЕсли;
	КонецЦикла;
	Если СтрокиМесяца.Количество() > 0 Тогда
		
		Прочее = "Сумма,Скидка,СуммаПФР,СкидкаПФР,СуммаФОМС,СкидкаФОМС,СуммаФСС,СкидкаФСС";
		БазыОПС = "БазаПФР,БазаПодземныеИВредныеРаботы,БазаТяжелыеИПрочиеРаботы,"
			+ "БазаПодземныеИВредныеРаботыОпасный,БазаТяжелыеИПрочиеРаботыОпасный,"
			+ "БазаПодземныеИВредныеРаботыВредный1,"
			+ "БазаТяжелыеИПрочиеРаботыВредный1,"
			+ "БазаПодземныеИВредныеРаботыВредный2,"
			+ "БазаТяжелыеИПрочиеРаботыВредный2,"
			+ "БазаПодземныеИВредныеРаботыВредный3,"
			+ "БазаТяжелыеИПрочиеРаботыВредный3,"
			+ "БазаПодземныеИВредныеРаботыВредный4,"
			+ "БазаТяжелыеИПрочиеРаботыВредный4";
		
		ПереченьБазВредныхВзносов = СтрРазделить(СтрЗаменить(БазыОПС, "БазаПФР,", ""), ",", Ложь);
		Ресурсы = СтрРазделить(Прочее + "," + БазыОПС + ",БазаФОМС,БазаФСС,БазаФСС_НС", ",", Ложь);
		
		МРОТ = СтрокиМесяца[0].РазмерМРОТ;
		ТребуетсяВыделятьМРОТ_ПФР = СтрокиМесяца[0].ТребуетсяВыделятьМРОТ_ПФР;
		ТребуетсяВыделятьМРОТ_ФОМС = СтрокиМесяца[0].ТребуетсяВыделятьМРОТ_ФОМС;
		ТребуетсяВыделятьМРОТ_ФСС = СтрокиМесяца[0].ТребуетсяВыделятьМРОТ_ФСС;
		
		Коэффициенты = ОбщегоНазначения.ВыгрузитьКолонку(СтрокиМесяца, "БазаПФР");
		РезультатыПФР = ЗарплатаКадрыКлиентСервер.РаспределитьПропорциональноКоэффициентам(МРОТ, Коэффициенты);
		Коэффициенты = ОбщегоНазначения.ВыгрузитьКолонку(СтрокиМесяца, "БазаФОМС");
		РезультатыОМС = ЗарплатаКадрыКлиентСервер.РаспределитьПропорциональноКоэффициентам(МРОТ, Коэффициенты);
		Коэффициенты = ОбщегоНазначения.ВыгрузитьКолонку(СтрокиМесяца, "БазаФСС");
		РезультатыФСС = ЗарплатаКадрыКлиентСервер.РаспределитьПропорциональноКоэффициентам(МРОТ, Коэффициенты);
		Коэффициенты = ОбщегоНазначения.ВыгрузитьКолонку(СтрокиМесяца, "Сумма");
		РезультатыСумма = ЗарплатаКадрыКлиентСервер.РаспределитьПропорциональноКоэффициентам(МРОТ, Коэффициенты);

		Для каждого ТекущаяСтрокаМесяца Из СтрокиМесяца Цикл
			ЗаполнитьЗначенияСвойств(НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы), ТекущаяСтрокаМесяца, , Прочее + ", " + БазыОПС + ", БазаФОМС, БазаФСС");
			Если РезультатыСумма = Неопределено Тогда
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , БазыОПС + ", БазаФОМС, БазаФСС, БазаФСС_НС");
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
			КонецЕсли;
			Если Не ТребуетсяВыделятьМРОТ_ПФР Или РезультатыПФР = Неопределено Тогда
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , СтрЗаменить(Прочее, ",СуммаПФР,СкидкаПФР", "") + ", БазаФОМС, БазаФСС, БазаФСС_НС");
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
			КонецЕсли;
			Если Не ТребуетсяВыделятьМРОТ_ФОМС Или РезультатыОМС = Неопределено Тогда
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , СтрЗаменить(Прочее, ",СуммаФОМС,СкидкаФОМС", "")  + ", " + БазыОПС + ", БазаФСС, БазаФСС_НС");
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
			КонецЕсли;
			Если Не ТребуетсяВыделятьМРОТ_ФСС Или РезультатыФСС = Неопределено Тогда
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , СтрЗаменить(Прочее, ",СуммаФСС,СкидкаФСС", "") + ", " + БазыОПС + ", БазаФОМС, БазаФСС_НС");
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
			КонецЕсли;
		КонецЦикла;
		
		Если РезультатыСумма <> Неопределено Тогда
			Для Индекс = 0 По СтрокиМесяца.ВГраница() Цикл
				
				ТекущаяСтрокаМесяца = СтрокиМесяца[Индекс];
				
				СтрокаОсновнойТариф = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(СтрокаОсновнойТариф, ТекущаяСтрокаМесяца, , "СуммаПФР, СкидкаПФР, СуммаФОМС, СкидкаФОМС, СуммаФСС, СкидкаФСС, " + БазыОПС + ", БазаФОМС, БазаФСС, БазаФСС_НС");
				СтрокаОсновнойТариф.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
				Если ТребуетсяВыделятьМРОТ_ПФР Тогда
					СтрокаОсновнойТариф.Сумма = ?(РезультатыПФР[Индекс] = 0, ТекущаяСтрокаМесяца.Сумма, РезультатыПФР[Индекс]);
				ИначеЕсли ТребуетсяВыделятьМРОТ_ФОМС Тогда
					СтрокаОсновнойТариф.Сумма = ?(РезультатыОМС[Индекс] = 0, ТекущаяСтрокаМесяца.Сумма, РезультатыОМС[Индекс]);
				ИначеЕсли ТребуетсяВыделятьМРОТ_ФСС Тогда
					СтрокаОсновнойТариф.Сумма = ?(РезультатыФСС[Индекс] = 0, ТекущаяСтрокаМесяца.Сумма, РезультатыФСС[Индекс]);
				Иначе
					Продолжить;
				КонецЕсли;
				Если ТребуетсяВыделятьМРОТ_ПФР Тогда
					СтрокаОсновнойТариф.СуммаПФР = ?(РезультатыПФР[Индекс] = 0, ТекущаяСтрокаМесяца.Сумма, РезультатыПФР[Индекс]);
				КонецЕсли;
				Если ТребуетсяВыделятьМРОТ_ФОМС Тогда
					СтрокаОсновнойТариф.СуммаФОМС = ?(РезультатыОМС[Индекс] = 0, ТекущаяСтрокаМесяца.Сумма, РезультатыОМС[Индекс]);
				КонецЕсли;
				Если ТребуетсяВыделятьМРОТ_ФСС Тогда
					СтрокаОсновнойТариф.СуммаФСС = ?(РезультатыФСС[Индекс] = 0, ТекущаяСтрокаМесяца.Сумма, РезультатыФСС[Индекс]);
				КонецЕсли;
				Если ТекущаяСтрокаМесяца.Сумма <> 0 И ТекущаяСтрокаМесяца.Скидка <> 0 Тогда
					СтрокаОсновнойТариф.Скидка = Формат(СтрокаОсновнойТариф.Сумма / ТекущаяСтрокаМесяца.Сумма * ТекущаяСтрокаМесяца.Скидка, "ЧДЦ=2");
					СтрокаОсновнойТариф.Сумма = СтрокаОсновнойТариф.Сумма + СтрокаОсновнойТариф.Скидка;
					Если ТребуетсяВыделятьМРОТ_ПФР И СтрокаОсновнойТариф.СуммаПФР <> 0 Тогда
						Если РезультатыПФР[Индекс] = 0 Тогда
							СтрокаОсновнойТариф.СкидкаПФР = ТекущаяСтрокаМесяца.СкидкаПФР;
						Иначе
							СтрокаОсновнойТариф.СкидкаПФР = Формат(СтрокаОсновнойТариф.СуммаПФР / ТекущаяСтрокаМесяца.СуммаПФР * ТекущаяСтрокаМесяца.СкидкаПФР, "ЧДЦ=2");
							СтрокаОсновнойТариф.СуммаПФР = СтрокаОсновнойТариф.СуммаПФР + СтрокаОсновнойТариф.СкидкаПФР;
						КонецЕсли;
					КонецЕсли;
					Если ТребуетсяВыделятьМРОТ_ФОМС И СтрокаОсновнойТариф.СуммаФОМС <> 0 Тогда
						Если РезультатыОМС[Индекс]= 0 Тогда
							СтрокаОсновнойТариф.СкидкаФОМС = ТекущаяСтрокаМесяца.СкидкаФОМС;
						Иначе
							СтрокаОсновнойТариф.СкидкаФОМС = Формат(СтрокаОсновнойТариф.СуммаФОМС / ТекущаяСтрокаМесяца.СуммаФОМС * ТекущаяСтрокаМесяца.СкидкаФОМС, "ЧДЦ=2");
							СтрокаОсновнойТариф.СуммаФОМС = СтрокаОсновнойТариф.СуммаФОМС + СтрокаОсновнойТариф.СкидкаФОМС;
						КонецЕсли;
					КонецЕсли;
					Если ТребуетсяВыделятьМРОТ_ФСС И СтрокаОсновнойТариф.СуммаФСС <> 0 Тогда
						Если РезультатыФСС[Индекс] = 0 Тогда
							СтрокаОсновнойТариф.СкидкаФСС = ТекущаяСтрокаМесяца.СкидкаФСС;
						Иначе
							СтрокаОсновнойТариф.СкидкаФСС = Формат(СтрокаОсновнойТариф.СуммаФСС / ТекущаяСтрокаМесяца.СуммаФСС * ТекущаяСтрокаМесяца.СкидкаФСС, "ЧДЦ=2");
							СтрокаОсновнойТариф.СуммаФСС = СтрокаОсновнойТариф.СуммаФСС + СтрокаОсновнойТариф.СкидкаФСС;
						КонецЕсли;
					КонецЕсли;
				КонецЕсли;
				
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , "СуммаПФР, СкидкаПФР, СуммаФОМС, СкидкаФОМС, СуммаФСС, СкидкаФСС, " + БазыОПС + ", БазаФОМС, БазаФСС, БазаФСС_НС");
				НоваяСтрока.Сумма = ТекущаяСтрокаМесяца.Сумма - СтрокаОсновнойТариф.Сумма;
				Если ТребуетсяВыделятьМРОТ_ПФР Тогда
					НоваяСтрока.СуммаПФР = ТекущаяСтрокаМесяца.СуммаПФР - СтрокаОсновнойТариф.СуммаПФР;
				КонецЕсли;
				Если ТребуетсяВыделятьМРОТ_ФОМС Тогда
					НоваяСтрока.СуммаФОМС = ТекущаяСтрокаМесяца.СуммаФОМС - СтрокаОсновнойТариф.СуммаФОМС;
				КонецЕсли;
				Если ТребуетсяВыделятьМРОТ_ФСС Тогда
					НоваяСтрока.СуммаФСС = ТекущаяСтрокаМесяца.СуммаФСС - СтрокаОсновнойТариф.СуммаФСС;
				КонецЕсли;
				Если ТекущаяСтрокаМесяца.Скидка <> 0 Тогда
					НоваяСтрока.Скидка = ТекущаяСтрокаМесяца.Скидка - СтрокаОсновнойТариф.Скидка;
					Если ТребуетсяВыделятьМРОТ_ПФР Тогда
						НоваяСтрока.СкидкаПФР = ТекущаяСтрокаМесяца.СкидкаПФР - СтрокаОсновнойТариф.СкидкаПФР;
					КонецЕсли;
					Если ТребуетсяВыделятьМРОТ_ФОМС Тогда
						НоваяСтрока.СкидкаФОМС = ТекущаяСтрокаМесяца.СкидкаФОМС - СтрокаОсновнойТариф.СкидкаФОМС;
					КонецЕсли;
					Если ТребуетсяВыделятьМРОТ_ФСС Тогда
						НоваяСтрока.СкидкаФСС = ТекущаяСтрокаМесяца.СкидкаФСС - СтрокаОсновнойТариф.СкидкаФСС;
					КонецЕсли;
				КонецЕсли;
				
			КонецЦикла;                                                                              
		КонецЕсли;
		Если ТребуетсяВыделятьМРОТ_ПФР И РезультатыПФР <> Неопределено Тогда
			Для Индекс = 0 По СтрокиМесяца.ВГраница() Цикл
				ТекущаяСтрокаМесяца = СтрокиМесяца[Индекс];
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , Прочее + ", БазаФОМС, БазаФСС, БазаФСС_НС");
				НоваяСтрока.БазаПФР = РезультатыПФР[Индекс];
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
				Для каждого Ресурс Из ПереченьБазВредныхВзносов Цикл
					Если ТекущаяСтрокаМесяца[Ресурс] = ТекущаяСтрокаМесяца.БазаПФР Тогда
						НоваяСтрока[Ресурс] = НоваяСтрока.БазаПФР
					КонецЕсли;
				КонецЦикла; 
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , Прочее + ", БазаФОМС, БазаФСС, БазаФСС_НС");
				НоваяСтрока.БазаПФР = ТекущаяСтрокаМесяца.БазаПФР - РезультатыПФР[Индекс];
				Для каждого Ресурс Из ПереченьБазВредныхВзносов Цикл
					Если ТекущаяСтрокаМесяца[Ресурс] = ТекущаяСтрокаМесяца.БазаПФР Тогда
						НоваяСтрока[Ресурс] = НоваяСтрока.БазаПФР
					КонецЕсли;
				КонецЦикла; 
			КонецЦикла;                                                                              
		КонецЕсли;
		Если ТребуетсяВыделятьМРОТ_ФОМС И РезультатыОМС <> Неопределено Тогда
			Для Индекс = 0 По СтрокиМесяца.ВГраница() Цикл
				ТекущаяСтрокаМесяца = СтрокиМесяца[Индекс];
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , Прочее + ", " + БазыОПС + ", БазаФСС, БазаФСС_НС");
				НоваяСтрока.БазаФОМС = РезультатыОМС[Индекс];
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , Прочее + ", " + БазыОПС + ", БазаФСС, БазаФСС_НС");
				НоваяСтрока.БазаФОМС = ТекущаяСтрокаМесяца.БазаФОМС - РезультатыОМС[Индекс];
			КонецЦикла;
		КонецЕсли;
		Если ТребуетсяВыделятьМРОТ_ФСС И РезультатыФСС <> Неопределено Тогда
			Для Индекс = 0 По СтрокиМесяца.ВГраница() Цикл
				ТекущаяСтрокаМесяца = СтрокиМесяца[Индекс];
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , Прочее + ", " + БазыОПС + ", БазаФОМС, БазаФСС_НС");
				НоваяСтрока.БазаФСС = РезультатыФСС[Индекс];
				НоваяСтрока.ВидТарифаСтраховыхВзносов = ОсновнойТариф;
				НоваяСтрока = НоваяСтрокаСОбнулениемРесурсов(Результат, Ресурсы);
				ЗаполнитьЗначенияСвойств(НоваяСтрока, ТекущаяСтрокаМесяца, , Прочее + ", " + БазыОПС + ", БазаФОМС, БазаФСС_НС");
				НоваяСтрока.БазаФСС = ТекущаяСтрокаМесяца.БазаФСС - РезультатыФСС[Индекс];
			КонецЦикла;
		КонецЕсли;
		
	КонецЕсли;
КонецПроцедуры

Процедура СформироватьВТОписаниеТарифов(НачалоПериода, ОкончаниеПериода, МенеджерВременныхТаблиц)
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = МенеджерВременныхТаблиц;
	Запрос.УстановитьПараметр("ДатаСтрахованияВременноПребывающих", ДатаСтрахованияВременноПребывающих());
	Запрос.УстановитьПараметр("ГодВведенияВзносовССуммПревышающихПредельную", Год(ДатаСтрахованияВременноПребывающих()));
	
	ПараметрыПостроения = ЗарплатаКадрыОбщиеНаборыДанных.ПараметрыПостроенияДляСоздатьВТИмяРегистраСрез();
	ПараметрыПостроения.ВсеЗаписи = Истина;
	
	ЗарплатаКадрыОбщиеНаборыДанных.СоздатьВТИмяРегистраСрезПоследних(
		"ТарифыСтраховыхВзносов",
		Запрос.МенеджерВременныхТаблиц,
		Ложь,
		ЗарплатаКадрыОбщиеНаборыДанных.ОписаниеФильтраДляСоздатьВТИмяРегистра("ВТПериодыРасширенныхСведенийОДоходах"),
		ПараметрыПостроения);
		
	ЗарплатаКадры.УничтожитьВТ(МенеджерВременныхТаблиц, "ВТРазмерыТарифныхСтавок", Истина);
	ЗарплатаКадры.УничтожитьВТ(МенеджерВременныхТаблиц, "ВТОписаниеТарифов", Истина);
		
	Запрос.Текст =
	"ВЫБРАТЬ РАЗЛИЧНЫЕ
	|	ТарифыСтраховыхВзносов.ПФР КАК ПФР,
	|	ТарифыСтраховыхВзносов.ПФРНакопительная КАК ПФРНакопительная,
	|	ТарифыСтраховыхВзносов.ФСС КАК ФСС,
	|	ТарифыСтраховыхВзносов.ФССсИностранцев КАК ФССсИностранцев,
	|	ТарифыСтраховыхВзносов.ФФОМС КАК ФФОМС,
	|	ТарифыСтраховыхВзносов.ТФОМС КАК ТФОМС,
	|	ТарифыСтраховыхВзносов.ВидТарифа КАК ВидТарифа,
	|	ТарифыСтраховыхВзносов.ПФР + ТарифыСтраховыхВзносов.ФСС + ТарифыСтраховыхВзносов.ФФОМС + ТарифыСтраховыхВзносов.ТФОМС КАК СуммарныйТариф,
	|	НАЧАЛОПЕРИОДА(ТарифыСтраховыхВзносов.Период, ГОД) КАК Период,
	|	ГОД(ТарифыСтраховыхВзносов.Период) КАК РасчетныйПериод
	|ПОМЕСТИТЬ ВТРазмерыТарифныхСтавок
	|ИЗ
	|	ВТТарифыСтраховыхВзносовСрезПоследних КАК ТарифыСтраховыхВзносов
	|
	|ИНДЕКСИРОВАТЬ ПО
	|	ВидТарифа,
	|	РасчетныйПериод
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Тарифы.ВидТарифа КАК ВидТарифа,
	|	Тарифы.РасчетныйПериод КАК РасчетныйПериод,
	|	ВЫБОР
	|		КОГДА Тарифы.Период < &ДатаСтрахованияВременноПребывающих
	|			ТОГДА ИСТИНА
	|		КОГДА Тарифы.СуммарныйТариф > ТарифДляИнвалидов.СуммарныйТариф
	|			ТОГДА ИСТИНА
	|		ИНАЧЕ ЛОЖЬ
