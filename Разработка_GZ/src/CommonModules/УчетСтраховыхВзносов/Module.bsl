			// - добавляем сторнирования по одному, начиная с самого раннего, используя в качестве образцов строки с данными без сторно,
			// - считаем суммы превышения с учетом очередного сторнирования,
			// - месячные суммы для новых сторно-строк определяем как разность превышения в новой строке и исходной строке без сторно,
			// - для последующих строк повторяем расчет нарастающего итога с учетом сторнирования.
			
			ТекстЗапроса =
			"ВЫБРАТЬ
			|	СведенияОДоходах.ФизическоеЛицо КАК ФизическоеЛицо,
			|	СведенияОДоходах.ГоловнаяОрганизация КАК ГоловнаяОрганизация,
			|	СведенияОДоходах.Период КАК Период,
			|	СведенияОДоходах.ПериодРегистрации КАК ПериодРегистрации,
			|	СведенияОДоходах.ЗаГодПФР КАК ЗаГод,
			|	СведенияОДоходах.ЗаМесяцПФР КАК ЗаМесяц,
			|	ВЫБОР
			|		КОГДА Предел.РазмерПФР = 0
			|			ТОГДА 0
			|		ИНАЧЕ ВЫБОР
			|				КОГДА СведенияОДоходах.ЗаГодПФР - Предел.РазмерПФР >= 0
			|					ТОГДА СведенияОДоходах.ЗаГодПФР - Предел.РазмерПФР
			|				ИНАЧЕ 0
			|			КОНЕЦ - ВЫБОР
			|				КОГДА ЕСТЬNULL(ОблагаемаяБазаПрошлогоМесяца.ЗаГодПФР, 0) - Предел.РазмерПФР >= 0
			|					ТОГДА ЕСТЬNULL(ОблагаемаяБазаПрошлогоМесяца.ЗаГодПФР, 0) - Предел.РазмерПФР
			|				ИНАЧЕ 0
			|			КОНЕЦ
			|	КОНЕЦ КАК СуммаПревысившаяПредел,
			|	Предел.РазмерПФР КАК Предел,
			|	ГОД(СведенияОДоходах.Период) КАК РасчетныйПериод
			|ИЗ
			|	ВТБазаНарастающимИтогом КАК СведенияОДоходах
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТЛицаСоСторнированиемВПериодПревышения КАК ЛицаСоСторнированиемВПериодПревышения
			|		ПО СведенияОДоходах.ФизическоеЛицо = ЛицаСоСторнированиемВПериодПревышения.ФизическоеЛицо
			|			И СведенияОДоходах.ГоловнаяОрганизация = ЛицаСоСторнированиемВПериодПревышения.ГоловнаяОрганизация
			|		ЛЕВОЕ СОЕДИНЕНИЕ ВТПредельнаяВеличинаБазыСтраховыхВзносовСрезПоследних КАК Предел
			|		ПО СведенияОДоходах.Период = Предел.Период
			|		ЛЕВОЕ СОЕДИНЕНИЕ ВТБазаНарастающимИтогом КАК ОблагаемаяБазаПрошлогоМесяца
			|		ПО СведенияОДоходах.ФизическоеЛицо = ОблагаемаяБазаПрошлогоМесяца.ФизическоеЛицо
			|			И СведенияОДоходах.ГоловнаяОрганизация = ОблагаемаяБазаПрошлогоМесяца.ГоловнаяОрганизация
			|			И (СведенияОДоходах.Период = ДОБАВИТЬКДАТЕ(ОблагаемаяБазаПрошлогоМесяца.Период, МЕСЯЦ, 1))
			|			И (ГОД(СведенияОДоходах.Период) = ГОД(ОблагаемаяБазаПрошлогоМесяца.Период))
			|			И (ОблагаемаяБазаПрошлогоМесяца.Период = ОблагаемаяБазаПрошлогоМесяца.ПериодРегистрации)
			|ГДЕ
			|	СведенияОДоходах.Период = СведенияОДоходах.ПериодРегистрации
			|
			|УПОРЯДОЧИТЬ ПО
			|	ГоловнаяОрганизация,
			|	РасчетныйПериод,
			|	ФизическоеЛицо,
			|	Период";
			Запрос.Текст = ТекстЗапросаПоВидуСтрахования(ТекстЗапроса, ВидСтрахования);	
			ВыборкаНарастающимИтогом = Запрос.Выполнить().Выбрать();
			
			Запрос.Текст =
			"ВЫБРАТЬ
			|	СведенияОДоходах.ФизическоеЛицо КАК ФизическоеЛицо,
			|	СведенияОДоходах.ГоловнаяОрганизация КАК ГоловнаяОрганизация,
			|	ГОД(СведенияОДоходах.Период) КАК РасчетныйПериод,
			|	СведенияОДоходах.Период КАК Период,
			|	СведенияОДоходах.ПериодРегистрации КАК ПериодРегистрации,
			|	ЕСТЬNULL(СУММА(БазаЗаМесяц.ЗаМесяц), 0) КАК ЗаМесяц
			|ИЗ
			|	ВТПериодыРасчетаПревышенияСМесяцемРегистрации КАК СведенияОДоходах
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТЛицаСоСторнированиемВПериодПревышения КАК ЛицаСоСторнированиемВПериодПревышения
			|		ПО (ЛицаСоСторнированиемВПериодПревышения.ПериодПревышения <= СведенияОДоходах.ПериодРегистрации)
			|			И СведенияОДоходах.ФизическоеЛицо = ЛицаСоСторнированиемВПериодПревышения.ФизическоеЛицо
			|			И СведенияОДоходах.ГоловнаяОрганизация = ЛицаСоСторнированиемВПериодПревышения.ГоловнаяОрганизация
			|		ЛЕВОЕ СОЕДИНЕНИЕ ВТБазаЗаМесяц КАК БазаЗаМесяц
			|		ПО СведенияОДоходах.Период = БазаЗаМесяц.Период
			|			И СведенияОДоходах.ПериодРегистрации = БазаЗаМесяц.ПериодРегистрации
			|			И СведенияОДоходах.ФизическоеЛицо = БазаЗаМесяц.ФизическоеЛицо
			|			И СведенияОДоходах.ГоловнаяОрганизация = БазаЗаМесяц.ГоловнаяОрганизация
			|ГДЕ
			|	СведенияОДоходах.Период < СведенияОДоходах.ПериодРегистрации
			|
			|СГРУППИРОВАТЬ ПО
			|	СведенияОДоходах.ФизическоеЛицо,
			|	СведенияОДоходах.ГоловнаяОрганизация,
			|	СведенияОДоходах.Период,
			|	СведенияОДоходах.ПериодРегистрации
			|
			|УПОРЯДОЧИТЬ ПО
			|	ГоловнаяОрганизация,
			|	РасчетныйПериод,
			|	ФизическоеЛицо,
			|	ПериодРегистрации,
			|	Период";
			
			ПериодыСторнирования = Запрос.Выполнить().Выгрузить();
			СуммыСторнирования = ПериодыСторнирования.Скопировать();
			
			ПериодыСторнирования.Свернуть("ПериодРегистрации,РасчетныйПериод,ГоловнаяОрганизация,ФизическоеЛицо");
			ПериодыСторнирования.Индексы.Добавить("ГоловнаяОрганизация,РасчетныйПериод,ФизическоеЛицо");
			СтруктураПоискаСторнирования = Новый Структура("ГоловнаяОрганизация,РасчетныйПериод,ФизическоеЛицо");
			СуммыСторнирования.Индексы.Добавить("ГоловнаяОрганизация,РасчетныйПериод,ФизическоеЛицо,ПериодРегистрации");
			СтруктураПоискаСуммСторнирования = Новый Структура("ГоловнаяОрганизация,РасчетныйПериод,ФизическоеЛицо,ПериодРегистрации");
			
			РасчетПревышения = ТаблицаСоСторнированием.СкопироватьКолонки();
			РасчетПревышения.Колонки.Добавить("ЗаГод", Новый ОписаниеТипов("Число"));
			
			Пока ВыборкаНарастающимИтогом.СледующийПоЗначениюПоля("ГоловнаяОрганизация") Цикл
				Пока ВыборкаНарастающимИтогом.СледующийПоЗначениюПоля("РасчетныйПериод") Цикл
					Предел = ВыборкаНарастающимИтогом.Предел;
					Пока ВыборкаНарастающимИтогом.СледующийПоЗначениюПоля("ФизическоеЛицо") Цикл
						
						// Получим суммы превышения до всех сторнирований.
						Пока ВыборкаНарастающимИтогом.Следующий() Цикл
							ЗаполнитьЗначенияСвойств(РасчетПревышения.Добавить(), ВыборкаНарастающимИтогом);
						КонецЦикла;
						
						// Дополним сторно-суммами. 
						ЗаполнитьЗначенияСвойств(СтруктураПоискаСторнирования, ВыборкаНарастающимИтогом);
						ПериодыСторнированияФизлица = ПериодыСторнирования.Скопировать(СтруктураПоискаСторнирования);
						Если ПериодыСторнированияФизлица.Количество() > 1 Тогда
							ПериодыСторнированияФизлица.Сортировать("ПериодРегистрации");
						КонецЕсли;
						Для каждого Период Из ПериодыСторнированияФизлица Цикл
							
							ТекущийПериодРегистрации = Период.ПериодРегистрации;
							
							// Выберем отсторнированные в текущем периоде регистрации суммы. 
							ЗаполнитьЗначенияСвойств(СтруктураПоискаСуммСторнирования, Период);
							СуммыСторнированияФизлица = СуммыСторнирования.Скопировать(СтруктураПоискаСуммСторнирования);
							Если СуммыСторнированияФизлица.Количество() > 1 Тогда
								СуммыСторнированияФизлица.Сортировать("Период");
							КонецЕсли;
							
							Если ВыборкаНарастающимИтогом.РасчетныйПериод = Год(ТекущийПериодРегистрации) Тогда
								// Вставляем сторно-строки перед основной строкой текущего периода регистрации.
								ИндексВставкиСторноСтроки = -1;
								Для каждого СтрокаРезультата Из РасчетПревышения Цикл
									ИндексВставкиСторноСтроки = ИндексВставкиСторноСтроки + 1;
									Если СтрокаРезультата.ПериодРегистрации > ТекущийПериодРегистрации Тогда
										ИндексВставкиСторноСтроки = ИндексВставкиСторноСтроки - 1;
										Прервать;
									КонецЕсли;
								КонецЦикла;
							Иначе
								// Добавляем сторно-строки в конец таблицы.
								ИндексВставкиСторноСтроки = РасчетПревышения.Количество();
							КонецЕсли;
							НакопленноеСторно = 0;
							УчтенноеСторно = 0;
							Для каждого СуммаСторнирования Из СуммыСторнированияФизлица Цикл
								
								ПериодСторнирования = СуммаСторнирования.Период;
								НакопленноеСторно = НакопленноеСторно + СуммаСторнирования.ЗаМесяц;
								
								ИндексПоследнейСтроки = РасчетПревышения.Количество() - 1;
								Для Индекс = 0 По ИндексПоследнейСтроки Цикл
									СтрокаРезультата = РасчетПревышения[ИндексПоследнейСтроки - Индекс];
									Если СтрокаРезультата.Период = ПериодСторнирования Тогда
										ИсходнаяСтрока = СтрокаРезультата;
										Прервать;
									КонецЕсли;
								КонецЦикла; 
								
								ДобавленнаяСтрока = РасчетПревышения.Вставить(ИндексВставкиСторноСтроки);
								ЗаполнитьЗначенияСвойств(ДобавленнаяСтрока, ИсходнаяСтрока);
								ДобавленнаяСтрока.ПериодРегистрации = ТекущийПериодРегистрации;
								ДобавленнаяСтрока.ЗаГод = СтрокаРезультата.ЗаГод + НакопленноеСторно;
								ДобавленнаяСтрока.СуммаПревысившаяПредел = Макс(0, ДобавленнаяСтрока.ЗаГод - Предел) - Макс(0, ИсходнаяСтрока.ЗаГод - Предел) - УчтенноеСторно;
								УчтенноеСторно = УчтенноеСторно + ДобавленнаяСтрока.СуммаПревысившаяПредел;
								
								ИндексВставкиСторноСтроки = ИндексВставкиСторноСтроки + 1;
							КонецЦикла;
							
							// Пересчитываем суммы превышения с учетом сторнирования для последующих строк.
							Для Индекс = ИндексВставкиСторноСтроки По РасчетПревышения.Количество() - 1 Цикл
								СтрокаРезультата = РасчетПревышения[Индекс];
								СтрокаРезультата.СуммаПревысившаяПредел = Макс(0, СтрокаРезультата.ЗаГод - Предел) - Макс(0, РасчетПревышения[Индекс - 1].ЗаГод - Предел);
							КонецЦикла;
							
						КонецЦикла; 
						
						// Переписываем в итоговую таблицу.
						ОбщегоНазначенияКлиентСервер.ДополнитьТаблицу(РасчетПревышения, ТаблицаСоСторнированием);
						РасчетПревышения.Очистить();
						
					КонецЦикла;
				КонецЦикла;
			КонецЦикла;
			
			// Собираем итоговую таблицу с суммами превышения предела, как по лицам со сторнированием, так и без.
			// Для строк превышения предела, по которым отсутствуют зарегистрированные в ИБ сведения о доходах, 
			// подбираем подходящие данные из базы других строк. 
			ТекстЗапроса =
			"ВЫБРАТЬ
			|	ТаблицаСоСторнированием.ФизическоеЛицо КАК ФизическоеЛицо,
			|	ТаблицаСоСторнированием.ГоловнаяОрганизация КАК ГоловнаяОрганизация,
			|	ТаблицаСоСторнированием.Период КАК Период,
			|	ТаблицаСоСторнированием.ПериодРегистрации КАК ПериодРегистрации,
			|	ТаблицаСоСторнированием.ЗаМесяц КАК ЗаМесяцПФР,
			|	ТаблицаСоСторнированием.Предел КАК РазмерПФР,
			|	ТаблицаСоСторнированием.СуммаПревысившаяПредел КАК СуммаПревысившаяПредел
			|ПОМЕСТИТЬ ВТТаблицаСоСторнированием
			|ИЗ
			|	&ТаблицаСоСторнированием КАК ТаблицаСоСторнированием
			|ГДЕ
			|	ТаблицаСоСторнированием.СуммаПревысившаяПредел <> 0
			|;
			|
			|////////////////////////////////////////////////////////////////////////////////
			|ВЫБРАТЬ
			|	ОблагаемаяБаза.ФизическоеЛицо КАК ФизическоеЛицо,
			|	ОблагаемаяБаза.ГоловнаяОрганизация КАК ГоловнаяОрганизация,
			|	ОблагаемаяБаза.Период КАК Период,
			|	ОблагаемаяБаза.ПериодРегистрации КАК ПериодРегистрации,
			|	ВЫБОР
			|		КОГДА Предел.РазмерПФР = 0
			|			ТОГДА 0
			|		КОГДА ОблагаемаяБаза.Период < ЛицаСПревышениемПредела.ПериодПревышения
			|			ТОГДА 0
			|		ИНАЧЕ ВЫБОР
			|				КОГДА ОблагаемаяБаза.ЗаГодПФР - Предел.РазмерПФР >= 0
			|					ТОГДА ОблагаемаяБаза.ЗаГодПФР - Предел.РазмерПФР
			|				ИНАЧЕ 0
			|			КОНЕЦ - ВЫБОР
			|				КОГДА ЕСТЬNULL(ОблагаемаяБазаПрошлогоМесяца.ЗаГодПФР, 0) - Предел.РазмерПФР >= 0
			|					ТОГДА ЕСТЬNULL(ОблагаемаяБазаПрошлогоМесяца.ЗаГодПФР, 0) - Предел.РазмерПФР
			|				ИНАЧЕ 0
			|			КОНЕЦ
			|	КОНЕЦ КАК СуммаПревысившаяПредел,
			|	Предел.РазмерПФР = 0
			|		ИЛИ ОблагаемаяБаза.МРОТзаМесяц = 0
			|		ИЛИ ОблагаемаяБаза.Период < ЛицаСПревышениемПредела.ПериодПревышения КАК МесяцБезПревышения,
			|	ОблагаемаяБаза.ЗаМесяцПФР КАК ЗаМесяцПФР,
			|	ОблагаемаяБаза.МРОТзаМесяц КАК МРОТзаМесяц,
			|	ЛОЖЬ КАК ДобавленнаяСтрока
			|ПОМЕСТИТЬ ВТРасчетПревышенияДохода
			|ИЗ
			|	ВТБазаНарастающимИтогом КАК ОблагаемаяБаза
			|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТЛицаСПревышениемПредела КАК ЛицаСПревышениемПредела
			|		ПО ОблагаемаяБаза.ФизическоеЛицо = ЛицаСПревышениемПредела.ФизическоеЛицо
			|			И ОблагаемаяБаза.ГоловнаяОрганизация = ЛицаСПревышениемПредела.ГоловнаяОрганизация
			|		ЛЕВОЕ СОЕДИНЕНИЕ ВТЛицаСоСторнированиемВПериодПревышения КАК ЛицаСоСторнированиемВПериодПревышения
			|		ПО ОблагаемаяБаза.ФизическоеЛицо = ЛицаСоСторнированиемВПериодПревышения.ФизическоеЛицо
			|			И ОблагаемаяБаза.ГоловнаяОрганизация = ЛицаСоСторнированиемВПериодПревышения.ГоловнаяОрганизация
			|		ЛЕВОЕ СОЕДИНЕНИЕ ВТПредельнаяВеличинаБазыСтраховыхВзносовСрезПоследних КАК Предел
			|		ПО ОблагаемаяБаза.Период = Предел.Период
			|		ЛЕВОЕ СОЕДИНЕНИЕ ВТБазаНарастающимИтогом КАК ОблагаемаяБазаПрошлогоМесяца
